{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "aubio-ledfx",
  "version-string": "0.5.0-alpha",
  "description": "aubio is a library for audio and music analysis, synthesis, and effects",
  "dependencies": [
    "pkgconf",
    {
      "name": "libsndfile",
      "default-features": false,
      "features": ["external-libs"]
    },
    "libsamplerate",
    "fftw3",
    {
      "name": "mpg123",
      "platform": "linux"
    },
    {
      "name": "mp3lame",
      "platform": "linux"
    },
    {
      "name": "opus",
      "platform": "linux"
    },
    {
      "name": "libogg",
      "platform": "linux"
    },
    {
      "name": "libvorbis",
      "platform": "linux"
    },
    {
      "name": "libflac",
      "platform": "linux"
    },
    {
      "name": "rubberband",
      "platform": "!linux"
    },
    {
      "name": "ffmpeg",
      "default-features": false,
      "features": ["avcodec", "avformat", "swresample"],
      "platform": "!linux"
    }
  ]
}
